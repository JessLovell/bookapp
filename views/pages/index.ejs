<!DOCTYPE html>
<html lang="en">
    <%- include('../layout/head') %>
  <body>
    <%- include('../layout/header') %>
    <main>
      <h2>Search by Title or Author</h2>

      <section class="book-list">
        <% results.forEach(book => { %>
          <li class="book-item">
            <img src="<%= book.img_url %>" />
            <h3> <%= book.title %></h3>
            <h4><%= book.author %></h4>
          </li>
        <% })%>
      </section>
    </main>
  </body>
</html>