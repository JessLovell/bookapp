<!DOCTYPE hmtl>
<html>
    <head></head>
    <body>
        <h1>Search Results</h1>
        <p><a href="/">Home</a></p>
        <ul>
            <% bookInfo.forEach(item => { %>
                <li><%= item.title%></li>
                <li><%= item.author%></li>
                <li><%= item.isbn%></li>
                <li><%= item.img_url%></li>
                <li><%= item.description%></li>
                <% }) %>
        </ul>
    </body>
</html>