<!DOCTYPE hmtl>
<html>
    <head></head>
    <body>
        <h1>Search Results</h1>
        <p><a href="/">Home</a></p>
        <ul>
            <% bookInfo.forEach(item => { %>
            <li class="book-item">
                <h2><%= item.title %></h2>
                <h2><%= item.author %></h2>
                <h3><%= item.isbn %></h3>
                <img src="<%= item.img_url %>" />
                <p><%= item.description %></p>
                <% }) %>
            </li>
        </ul>
    </body>
</html>